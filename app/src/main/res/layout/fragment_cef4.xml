<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cef4">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#4A587B"
        android:gravity="center_horizontal"
        android:text="CEF ENTRY"
        android:textSize="20dp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="25dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/textbox"
            android:text=" Other Valuable Details "
            android:textSize="15dp" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp">

            <TableRow android:layout_height="30dp">

                <TextView
                    android:id="@+id/textView51"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Residence \nStatus :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <Spinner
                        android:id="@+id/resstatus"
                        android:layout_width="150dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="50dp"
                        android:backgroundTint="#B79C70"
                        android:backgroundTintMode="screen" />
                </LinearLayout>
            </TableRow>

            <TableRow
                android:layout_height="30dp"
                android:layout_marginTop="12dp">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Home \nOwnership :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <Spinner
                        android:id="@+id/homeowner"
                        android:layout_width="150dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="50dp"
                        android:backgroundTint="#B79C70"
                        android:backgroundTintMode="screen" />
                </LinearLayout>
            </TableRow>

            <TableRow
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Vehicles Owned\n (In Nos.) :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView18"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:text="Four-Wheeler:"
                            android:textSize="13dp" />

                        <EditText
                            android:id="@+id/fourwheel"
                            android:layout_width="30dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="20dp"
                            android:background="@drawable/textbox2"
                            android:inputType="textPersonName"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            android:textSize="15dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/textView1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:text="Two-Wheeler:"
                            android:textSize="13dp" />

                        <EditText
                            android:id="@+id/twowheel"
                            android:layout_width="30dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="20dp"
                            android:background="@drawable/textbox2"
                            android:inputType="textPersonName"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            android:textSize="15dp" />
                    </LinearLayout>
                </LinearLayout>

            </TableRow>

            <TableRow
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp">

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Existing \nInvestment :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <Spinner
                        android:id="@+id/existinvest"
                        android:layout_width="150dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="50dp"
                        android:backgroundTint="#B79C70"
                        android:backgroundTintMode="screen" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="50dp"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/einv"
                            android:layout_width="30dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="20dp"
                            android:background="@drawable/textbox2"
                            android:inputType="textPersonName"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            android:textSize="15dp" />

                        <ImageButton
                            android:id="@+id/investimgbtn"
                            android:layout_width="30dp"
                            android:layout_height="20dp"
                            android:layout_weight="1"
                            android:src="@android:drawable/ic_input_add" />

                        <Button
                            android:id="@+id/investview"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:layout_gravity="center"
                            android:background="@drawable/button3"
                            android:text="View" />
                    </LinearLayout>
                </LinearLayout>
            </TableRow>

            <TableRow
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Existing \nInsurance :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <Spinner
                        android:id="@+id/existinsurance"
                        android:layout_width="150dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="50dp"
                        android:backgroundTint="#B79C70"
                        android:backgroundTintMode="screen" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="50dp"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/einsure"
                            android:layout_width="30dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="20dp"
                            android:background="@drawable/textbox2"
                            android:inputType="textPersonName"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            android:textSize="15dp" />

                        <ImageButton
                            android:id="@+id/insureimagebtn"
                            android:layout_width="30dp"
                            android:layout_height="20dp"
                            android:layout_weight="1"
                            android:src="@android:drawable/ic_input_add" />

                        <Button
                            android:id="@+id/insureview"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:layout_gravity="center"
                            android:background="@drawable/button3"
                            android:text="View" />
                    </LinearLayout>
                </LinearLayout>
            </TableRow>
            <TableRow
                android:layout_height="30dp"
                android:layout_marginTop="18dp">

                <TextView
                    android:id="@+id/textView50"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="First Business :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                  <Button
                      android:layout_width="60dp"
                      android:layout_height="25dp"
                      android:id="@+id/firstbusiness"
                      android:text="Select"
                      android:textAlignment="center"
                      android:gravity="center"
                      android:layout_marginLeft="50dp"
                      android:background="@drawable/button3"/>
                </LinearLayout>
            </TableRow>
            <TableRow
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp">

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Investment\nStrategies :"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <Spinner
                        android:id="@+id/investstrat"
                        android:layout_width="150dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="50dp"
                        android:backgroundTint="#B79C70"
                        android:backgroundTintMode="screen" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:layout_marginLeft="50dp">

                        <EditText
                            android:id="@+id/invstratext"
                            android:layout_width="30dp"
                            android:layout_height="17dp"
                            android:layout_marginLeft="20dp"
                            android:background="@drawable/textbox2"
                            android:inputType="textPersonName"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            android:textSize="15dp" />

                        <ImageButton
                            android:id="@+id/invstratimagebtn"
                            android:layout_width="30dp"
                            android:layout_height="20dp"
                            android:layout_weight="1"
                            android:src="@android:drawable/ic_input_add" />

                        <Button
                            android:id="@+id/investstratview"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:textAlignment="center"
                            android:textSize="12dp"
                            android:layout_gravity="center"
                            android:background="@drawable/button3"
                            android:text="View" />
                    </LinearLayout>
                </LinearLayout>
            </TableRow>
            <TableRow android:layout_height="wrap_content"
                android:layout_marginTop="15dp">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Advisor's \nRemark :"
                    android:textSize="15dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/advremark"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="50dp"
                        android:textColor="@color/black"
                        android:background="@drawable/textbox2"
                        android:inputType="textMultiLine"
                        android:textSize="15dp"
                        android:lines="5"
                        android:textColorHint="#808080"
                        />
                </LinearLayout>
            </TableRow>
                 <LinearLayout android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                     android:orientation="horizontal">
                     <TextView
                         android:id="@+id/tnc"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Term's and Conditions:"
                         android:layout_marginTop="10dp"
                         android:textColor="#4A587B"
                         android:textSize="15dp" />
                     <TextView
                         android:id="@+id/preview"
                         android:layout_marginLeft="50dp"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Preview"
                         android:layout_marginTop="10dp"
                         android:textColor="#4A587B"
                         android:textSize="15dp" />
                 </LinearLayout>


        </TableLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/cef4prev"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/button2"
                android:text="Prev" />

            <Button
                android:id="@+id/send"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="20dp"
                android:text="send" />

            <Button
                android:id="@+id/cef4add"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/button2"
                android:text="Add" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>